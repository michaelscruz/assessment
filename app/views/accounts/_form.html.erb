<div class="jumbotron writing no-padding">
  <div class="container">
  </div>
</div>

<h1>Your Subdomain at OakrootAssessments</h1>

<div class="row">
  <div class="well col-sm-8 col-md-offset-2">

    <%= form_for(@account, url: accounts_path(creating_test: @creating_test), method: :post) do |f| %>

      <div class="form-group">
        <%= f.label :name %><br>
        <%= f.text_field :name, autofocus: true, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :subdomain %><br>
        <%= f.text_field :subdomain, autofocus: true, class: "form-control" %>
      </div>

      <div class="actions">
        <%= f.submit "Next", class: "btn btn-success btn-lg col-xs-12" %>
      </div>
    <% end %>

  </div>
</div>